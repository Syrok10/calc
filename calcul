+      <DIV ALIGN=LEFT><SMALL><FONT COLOR=000000>
+            </FONT></SMALL></DIV>
+        <INPUT TYPE=text NAME="view" SIZE=30 action="compute(this)"><BR>
+      <HR>
+    </TD>
+  </TR>
+  <TR>
+    <TD COLSPAN=2><DIV ALIGN=RIGHT>
+        <INPUT TYPE=button VALUE="  7  " onClick="getNum(this.form, seven)"> 
